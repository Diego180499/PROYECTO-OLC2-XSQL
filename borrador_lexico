EXPRESIONES REGULARES   

    ID = @ [a-zA-Z][a-zA-Z0-1]*
    entero = [0-9]+
    decimal = [0-9]+\.[0-9]+
    SEMICOLON = ';'
    NOMBRE = [a-zA-Z]+
    L_PAREN = '('
    R_PAREN = ')'
    COMMA = ','
    ASSIGN = '='
    EQUALS = '=='
    NOT_EQ = '!='
    LESS_THAN = '<'
    GREATER_THAN = '>'
    LESS_EQ = '<='
    GREATER_EQ = '>='
    AND = \&\&
    OR = \|\|
    NOT = \!
    PLUS = '+'
    MINUS = '-'
    TIMES = '*'
    DIVIDE = '/'
    
    
    





